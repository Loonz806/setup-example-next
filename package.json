    "@testing-library/cypress": "^8.0.7",
    "cypress": "^10.11.0",
    "cypress-axe": "^1.4.0",
    "eslint-plugin-cypress": "^2.12.1",
    "identity-obj-proxy": "^3.0.0",
    "json-server": "^0.17.2",
